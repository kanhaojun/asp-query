<configuration>

    <system.web>
      <compilation debug="true" targetFramework="4.6" />
      <httpRuntime targetFramework="4.6" />
    </system.web>
  
	<appSettings>
		<add key="ValidationSettings:UnobtrusiveValidationMode"
			value="None" />
	</appSettings>
	
    <connectionStrings>
		<add name="ConnectionString" connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\Product.mdf;Integrated Security=True"
		providerName="System.Data.SqlClient" />
	</connectionStrings>
</configuration>
